{
  "name": "lottery-facility",
  "version": "0.0.1-alpha.9",
  "description": "Some APIs and CLI tools for lottery analysis and for building smart lottery systems in Node.js.",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/lotteryfacility-webbundle.umd.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/lotteryfacility-webbundle.umd.js",
      "node": "./dist/lotteryfacility-nodebundle.umd.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "bin/",
    "dist/",
    "lib/"
  ],
  "private": false,
  "engines": {
    "node": ">=22.11.0"
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib",
    "build": "./build",
    "doc": "./doc",
    "test": "./test"
  },
  "scripts": {
    "clean": "rimraf build dist lib coverage",
    "copy-files": "copyfiles -f src/**/*.txt src/**/*.csv src/**/*.html src/**/*.css ./lib",
    "lint": "eslint",
    "prebuild": "npm run lint",
    "build:tsc": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "build:webpack": "webpack --mode=production",
    "build": "npm run clean && npm run build:tsc && npm run copy-files && npm run build:webpack",
    "start:dev": "webpack --mode=development --watch --progress",
    "postbuild": "",
    "test": "jest --config=jest.config.ts --rootDir . --forceExit --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test0": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "flash": "./bin/flash-cli.mjs",
    "difference": "./bin/difference-cli.mjs",
    "complement": "./bin/complement-cli.mjs",
    "translate": "./bin/translate-cli.mjs",
    "combination": "./bin/combination-cli.mjs",
    "cartesian_product": "./bin/cartesian_product-cli.mjs",
    "filter": "./bin/filter-cli_1.mjs",
    "euromillions": "./bin/euromillions-cli.mjs",
    "kenoFR": "./bin/kenoFR-cli.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalyre/lottery-facility.git"
  },
  "keywords": [
    "lottery",
    "gambling",
    "betting",
    "fdj",
    "wheeling",
    "combination",
    "EuroMillions",
    "MegaMillions",
    "Powerball",
    "EuroJackpot",
    "Lotto",
    "Keno",
    "jackpot",
    "cash",
    "money",
    "fortune",
    "chance",
    "luck",
    "random",
    "random generator",
    "random engine",
    "bookmaker",
    "roulette"
  ],
  "author": "Claude Lalyre <claude.lalyre@gmail.com>",
  "contributors": [
    "Claude Lalyre <claude.lalyre@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lalyre/lottery-facility/issues",
    "email": "claude.lalyre@gmail.com"
  },
  "homepage": "https://github.com/lalyre/lottery-facility#readme",
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "babel-loader": "^9.2.1",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "ansi-colors": "^4.1.3",
    "bent": "^7.3.12",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "fs-extra": "^11.2.0",
    "js-sha256": "^0.11.0",
    "jszip": "^3.10.1",
    "meow": "^13.2.0",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "sequelize": "^6.37.5",
    "sqlite": "^5.1.1"
  }
}
