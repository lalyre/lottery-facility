{
  "name": "@claudelalyre/lottery-facility",
  "version": "0.0.0",
  "description": "Some APIs for designing lottery cracking systems.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "copy-files": "copyfiles -f src/**/*.json src/**/*.txt ./dist",
    "lint:ts": "tslint -c tslint.json -p ./tsconfig.json \"src/**/*.ts\" -t stylish -e \"node_modules/**/*.ts\" -e \"**/*.d.ts\"",
    "tsc": "tsc",

    "clean": "rimraf build dist lib",

    "prebuild": "npm run clean && exit 0",
    "build": "tsc",
    "postbuild": "npm run copy-files",

    "test": "jest --config=jest.config.json",
    "test0": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalyre/lottery-facility.git"
  },
  "keywords": [
    "lottery",
    "gambling",
    "fdj",
    "wheeling",
    "combination",
    "euromillions",
    "lotto",
    "keno",
    "jackpot",
    "cash",
    "money",
    "fortune",
    "chance"
  ],
  "author": "Claude Lalyre <claude.lalyre@gmail.com>",
  "contributors": [
    "Claude Lalyre <claude.lalyre@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lalyre/lottery-facility/issues"
  },
  "homepage": "https://github.com/lalyre/lottery-facility#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5"
  }
}
