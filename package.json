{
  "name": "lottery-facility",
  "version": "0.0.1-alpha.9",
  "description": "Some APIs and CLI tools for designing lottery cracking systems.",
  "main": "./dist/lotteryfacility-nodebundle.umd.js",
  "exports": {
    ".": "./dist/lotteryfacility-nodebundle.umd.js"
  },
  "types": "main.d.ts",
  "typesVersions": {
    ">=5.0": {
      "*": [
        "ts5.0/*"
      ]
    }
  },
  "files": [
    "bin/",
    "dist/**.json",
    "dist/**.txt",
    "dist/**.csv",
    "dist/**.js",
    "ts5.0/"
  ],
  "type": "commonjs",
  "private": false,
  "engines": {
    "node": ">=12.22.0"
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib",
    "build": "./build",
    "doc": "./doc",
    "test": "./test"
  },
  "scripts": {
    "copy-files": "copyfiles -f src/**/*.json src/**/*.txt src/**/*.csv src/**/*.html src/**/*.css ./lib",
    "lint": "eslint ./src --ext .ts --ext .tsx",
    "tsc": "tsc",
    "clean": "rimraf build dist lib coverage types ts4.4 ts4.5 ts4.6 ts4.7 ts4.8 ts4.9 ts5.0",
    "prebuild": "npm run clean && npm run lint",
    "start:dev": "webpack --mode=development --watch --progress",
    "build": "tsc",
    "postbuild": "npm run copy-files && webpack --mode=production",
    "test": "jest --config=jest.config.ts --rootDir . --forceExit --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test0": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "flash": "./bin/flash-cli.mjs",
    "combination": "./bin/combination-cli.mjs",
    "translate": "./bin/translate-cli.mjs",
    "difference": "./bin/difference-cli.mjs",
    "filter": "./bin/filter-cli.mjs",
    "euromillions_draws": "./bin/euromillions-cli.mjs",
    "kenoFR_draws": "./bin/kenoFR-cli.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalyre/lottery-facility.git"
  },
  "keywords": [
    "lottery",
    "gambling",
    "betting",
    "fdj",
    "wheeling",
    "combination",
    "euromillions",
    "lotto",
    "keno",
    "jackpot",
    "cash",
    "money",
    "fortune",
    "chance",
    "luck",
    "random",
    "random generator",
    "random engine",
    "bookmaker",
    "roulette"
  ],
  "author": "Claude Lalyre <claude.lalyre@gmail.com>",
  "contributors": [
    "Claude Lalyre <claude.lalyre@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lalyre/lottery-facility/issues",
    "email": "claude.lalyre@gmail.com"
  },
  "homepage": "https://github.com/lalyre/lottery-facility#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.20",
    "@babel/runtime": "^7.22.15",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "bent": "^7.3.12",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "fs-extra": "^11.1.1",
    "js-sha256": "^0.10.1",
    "jszip": "^3.10.1",
    "meow": "^12.1.1",
    "path": "^0.12.7",
    "readline": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "es2021": true
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0
    }
  }
}
