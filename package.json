{
  "name": "lottery-facility",
  "version": "0.0.1-alpha.8",
  "description": "Some APIs for designing lottery cracking systems.",
  "main": "./dist/lotteryfacility-nodebundle.umd.js",
  "exports": {
    ".": "./dist/lotteryfacility-nodebundle.umd.js"
  },
  "types": "main.d.ts",
  "typesVersions": {
    ">=4.9": {
      "*": [
        "ts4.9/*"
      ]
    }
  },
  "files": [
    "bin/",
    "dist/**.json",
    "dist/**.txt",
    "dist/**.csv",
    "dist/**.js",
    "ts4.9/"
  ],
  "type": "mmmmmmmmmmmmmodule",
  "private": false,
  "engines": {
    "node": ">=12.19.0"
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib",
    "build": "./build",
    "doc": "./doc",
    "test": "./test"
  },
  "scripts": {
    "copy-files": "copyfiles -f src/**/*.json src/**/*.txt src/**/*.csv src/**/*.html src/**/*.css ./lib",
    "lint:ts": "tslint -c tslint.json -p ./tsconfig.json \"src/**/*.ts\" -t stylish -e \"node_modules/**/*.ts\" -e \"**/*.d.ts\"",
    "tsc": "tsc",
    "clean": "rimraf build dist lib coverage types ts4.7 ts4.8 ts4.9",
    "prebuild": "npm run clean && npm run lint:ts",
    "start:dev": "webpack --mode=development --watch --progress",
    "build": "tsc",
    "postbuild": "npm run copy-files && webpack --mode=production",
    "test": "jest --config=jest.config.ts --rootDir . --forceExit --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test0": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "flash": "./bin/flash-cli.js",
    "combination": "./bin/combination-cli.js",
    "translate": "./bin/translate-cli.js",
    "difference": "./bin/difference-cli.js",
    "filter": "./bin/filter-cli.js",
    "euromillions_draws": "./bin/euromillions_draws-cli.js",
    "kenoFR_draws": "./bin/kenoFR_draws-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalyre/lottery-facility.git"
  },
  "keywords": [
    "lottery",
    "gambling",
    "betting",
    "fdj",
    "wheeling",
    "combination",
    "euromillions",
    "lotto",
    "keno",
    "jackpot",
    "cash",
    "money",
    "fortune",
    "chance",
    "luck",
    "random",
    "random generator",
    "random engine",
    "bookmaker",
    "roulette"
  ],
  "author": "Claude Lalyre <claude.lalyre@gmail.com>",
  "contributors": [
    "Claude Lalyre <claude.lalyre@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lalyre/lottery-facility/issues",
    "email": "claude.lalyre@gmail.com"
  },
  "homepage": "https://github.com/lalyre/lottery-facility#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.179",
    "babel-preset-env": "^1.7.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^9.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "bent": "^7.3.12",
    "chalk": "^4.1.2",
    "cli-progress": "^3.10.0",
    "fs": "^0.0.1-security",
    "js-sha256": "^0.9.0",
    "jszip": "^3.7.1",
    "meow": "^7.1.1",
    "path": "^0.12.7",
    "readline": "^1.3.0"
  }
}
